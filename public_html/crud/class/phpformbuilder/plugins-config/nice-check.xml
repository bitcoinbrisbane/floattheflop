<?xml version="1.0" encoding="UTF-8"?>
<root>
    <default>
        <includes>
            <!--
                Path to css and js files into plugins folder.
                If your files are not in plugins folder, use absolute paths.
            -->
            <js>
                <file>nice-check/dist/js/jquery.nice-check.min.js</file>
            </js>
            <css>
                <file>nice-check/dist/css/nice-check-%skin%.min.css</file>
            </css>
        </includes>
        <js_code>
<![CDATA[   $('%selector%').niceCheck();]]>
        </js_code>
    </default>
</root>
